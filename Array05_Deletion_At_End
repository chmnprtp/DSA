//Deletion at end

#include<iostream>
using namespace std;

int main()
{
    int size;
    cout<<"Enter the size of an array"<<endl;
    cin>>size;
    int arr[size];
    
    cout<<"enter the value for an array"<<endl;
    for(int i=0; i<size; i++)
    {
        cin>>arr[i];
    }
    
    //Deletion at end
    size--;
    
    
    
    //print
    for(int i=0; i<size; i++)
    {
        cout<<" ";
        cout<<arr[i];
    }
}
