//Deletion at beg

#include<iostream>
using namespace std;

int main()

{
    int size;
    cout<<"enter the size of an array";
    cin>>size;
    int arr[size];
    
    cout<<"Enter the value for an array";
    for(int i=0; i<size; i++)
    {
        cin>>arr[i];
    }
    
    
    
    //Delete at beg
     for(int i=0; i<size; i++)
    {
        arr[i] = arr[i+1];
    }
    size--;
    
    //Print after delete at beg
    for(int i=0; i<size; i++)
    {
        cout<<arr[i];
    }
    
}
